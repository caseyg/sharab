{{#section 'title'}}
Wines
{{/section}}

<h1>Wines</h1>

<a href="/wines/new">
Add New Wine
</a>

<table>
<thead>
<tr>
<th>Name</th>
<th>Maker</th>
<th>Year</th>
<th>Tag</th>
</tr>
</thead>

<tbody>

{{#each wines}}
<tr>
<td>{{this.name}}</td>
<td>{{this.maker}}</td>
<td>{{this.vintage}}</td>
<td><a href="wines/{{this.tag}}">{{this.tagName}}</a></td>
</tr>
{{/each}}

</tbody>
</table>
