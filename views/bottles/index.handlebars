{{#section 'title'}}
Bottles
{{/section}}

<h1>Bottles</h1>

<a href="/bottles/new">
Add New Bottle
</a>

<table>
<thead>
<tr>
<th>Wine</th>
<th>Location</th>
</tr>
</thead>

<tbody>

{{#each bottles}}
<tr>
<td><a href="/wines/{{this.wine.tag}}">{{this.wine.name}}</a></td>
<td>{{this.location}}</td>
</tr>
{{/each}}

</tbody>
</table>
